extends ./layout.jade

block vars
  - var title = 'Dashboard'

block content
  h1 Dashboard

  if table[0] != undefined
    p.
      Displaying matches...
    table(border='1')
      thead
        tr
          th Name
          th Location
          th Room Number
          th Description
          th Datasheet
          th Item Number
        //-tr
          //-for val, key in table[0]
            //-th #{key}
      tbody
        for tuple in table
          tr
            for field in tuple
              td= field
  else
    p.
      No matches found. 

